-- MATHEMATIC FUNCTIONS â€“ 20 Questions
-- Find sine value of an angle.
SELECT sin(30) from dual;
-- Find cosine value of an angle.
SELECT cos(30) from dual;
-- Find tangent value.
select tan(30) from dual;
-- Convert degrees to radians.
select 30*3.14/180 FROM DUAL;
-- Convert radians to degrees.
select 30*180/3.14 FROM DUAL;
-- Find logarithm (base 10) of a number.
SELECT LOG(10,100) FROM DUAL;
-- Find natural log of a number.
SELECT LN(100) FROM DUAL;
-- Find square of a number.
SELECT POWER(10,2) FROM DUAL;
-- Find cube of a number.
SELECT POWER(10,3) FROM DUAL;
-- Find greatest value among three numbers.
SELECT GREATEST(11,2,3) FROM DUAL;
-- Find least value among three numbers.
SELECT LEAST(11,2,3) FROM DUAL;
-- Calculate variance of sales.
SELECT VARIANCE(QUANTITY*UNIT_PRICE) FROM ORDERS;
-- Calculate standard deviation of sales.
SELECT STDDEV(QUANTITY*UNIT_PRICE) FROM ORDERS;
-- Find average deviation.
SELECT AVG(ABS((Quantity*Unit_Price) -(SELECT AVG(Quantity*Unit_Price) FROM Orders))) FROM Orders;
-- Calculate geometric mean.
-- Calculate harmonic mean.
SELECT COUNT(*) / SUM(1/(Quantity*Unit_Price)) FROM Orders;
-- Find sum of squares.
SELECT SUM(POWER(Quantity*Unit_Price,2)) FROM Orders;
-- Calculate correlation between two columns.
SELECT CORR(Quantity, Unit_Price) AS Correlation FROM Orders;
-- Calculate regression slope.
SELECT REGR_SLOPE(Unit_Price, Quantity) AS Regression_Slope FROM Orders;